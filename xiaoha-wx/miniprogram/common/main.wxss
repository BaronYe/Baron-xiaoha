}

.tj-popup {
  background: #fff;
  position: fixed;
  width: 100%;
  z-index: 1;
  -webkit-transition: all 1s linear;
  transition: all 1s linear;
}

.tj-popup.is-top {
  left: 0;
  top: 0;
}

.tj-popup.move-enter-active,
.tj-popup.move-leave-active {
  -webkit-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
}

.tj-popup.move-enter,
.tj-popup.move-leave-active {
  -webkit-transform: translate3d(0, -100%, 0);
  transform: translate3d(0, -100%, 0);
}

.tj-popup.is-right {
  width: 70%;
  overflow-y: auto;
  height: 100%;
  right: 0;
  top: 0;
}

.tj-popup.move-enter-active,
.tj-popup.move-leave-active {
  -webkit-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
}

.tj-popup.move-enter,
.tj-popup.move-leave-active {
  -webkit-transform: translate3d(100%, 0, 0);
  transform: translate3d(100%, 0, 0);
}

.tj-popup.is-bottom {
  left: 0;
  bottom: 0;
}

.tj-popup.move-enter-active,
.tj-popup.move-leave-active {
  -webkit-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
}

.tj-popup.move-enter,
.tj-popup.move-leave-active {
  -webkit-transform: translate3d(0, 100%, 0);
  transform: translate3d(0, 100%, 0);
}

.tj-popup .tj-popup__main {
  max-height: 300rpx;
  overflow-y: auto;
}

.tj-popup.is-left {
  left: 0;
  top: 0;
}

.tj-popup.move-enter-active,
.tj-popup.move-leave-active {
  -webkit-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
}

.tj-popup.move-enter,
.tj-popup.move-leave-active {
  -webkit-transform: translate3d(-100%, 0, 0);
  transform: translate3d(-100%, 0, 0);
}

.tj-popup .tj-popup__head {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  padding: 30rpx 30rpx 0;
}

.tj-popup .tj-popup__title {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
