  -webkit-flex: 1;
  flex: 1;
  text-align: right;
}

.tj-popup .tj-popup__main {
  padding: 30rpx;
}

.tj-collapse {
  padding: 30rpx;
  background: #fff;
  overflow: hidden;
}

.tj-collapse .tj-collapse__head {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}

.tj-collapse .tj-collapse__title {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  font-size: 16px;
}

.tj-collapse .tj-collapse__btn {
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 200rpx;
  flex: 0 0 200rpx;
  color: #999;
  text-align: right;
}

.tj-collapse .tj-collapse__btn .tj-collapse__txt {
  color: #00c9a7;
}

.tj-collapse .tj-collapse__btn .tj-icon {
