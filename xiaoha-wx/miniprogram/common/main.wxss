.tj-row {
  width: 100%;
  overflow: hidden;
}

.tj-row .tj-row__inner:before {
  content: '';
  display: table;
}

.tj-row .tj-row__inner::after {
  content: '';
  display: table;
  clear: both;
}

.is-flex .tj-row__inner {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
}

.is-flex .tj-row__inner:before {
  content: '';
  display: table;
}

.is-flex .tj-row__inner::after {
  content: '';
  display: table;
  clear: both;
}

.tj-col {
  box-sizing: border-box;
  line-height: 38rpx;
}

.tj-col.is-vertical:after {
  content: '';
  width: 0;
