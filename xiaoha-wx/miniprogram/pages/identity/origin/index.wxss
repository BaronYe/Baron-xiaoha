.identity-origin{
  height: 100vh;
}
.identity-origin>view:nth-child(1) {
  position: relative;
}

.identity-origin>view:nth-child(1)>image:nth-child(1) {
  display: block;
  width: 100%;
  height: 1063rpx;
}

.identity-origin>view:nth-child(1)>image:nth-child(2) {
  position: absolute;
  left: 50%;
  bottom: 0;
  transform: translate(-50%, 50%);
  width: 214rpx;
  height: 214rpx;
}

.identity-origin>view:nth-child(2) {
  padding: 100rpx 80rpx 56rpx;
}

.identity-origin>view:nth-child(2)>view {
  line-height: 50rpx;
}

.identity-origin>view:nth-child(2)>view:nth-child(3) {
  line-height: 40rpx;
}

.identity-origin>view:nth-child(3) {
  width: 340rpx;
  line-height: 88rpx;
  color: #fff;
  background: linear-gradient(90deg, #02BFFF 0%, #2759FF 100%);
  border-radius: 44rpx;
  margin: auto;
}