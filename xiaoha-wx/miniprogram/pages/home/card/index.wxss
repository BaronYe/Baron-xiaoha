canvas {
  position: fixed;
  top: 0;
  left: 500px;
}

.home-card {
  padding: 0 30rpx 60rpx;
}

.home-card .partition {
  display: flex;
  justify-content: space-between;
  line-height: 80rpx;
  height: 80rpx;
  margin-top: 20rpx;
}

.home-card .partition image {
  width: 36rpx;
  height: 37rpx;
  padding: 20rpx;
}

.home-card>.card {
  position: relative;
  width: 690rpx;
  height: 414rpx;
  box-shadow: 0px 0px 20px 0px rgba(4, 4, 155, 0.2);
  border-radius: 30rpx;
  box-sizing: border-box;
  padding: 30rpx 30rpx 48rpx;
  color: #fff;
}

.home-card>.card>image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  border-radius: 30rpx;
  z-index: -1;
}

.home-card>.card>view:nth-of-type(1) {
  display: flex;
  padding-bottom: 40rpx;
}

.home-card>.card>view:nth-of-type(1)>view:nth-of-type(1) {
  width: 140rpx;
  height: 140rpx;
  margin-right: 18rpx;
}

.home-card>.card>view:nth-of-type(1)>view:nth-of-type(1)>image {
  width: 100%;
  height: 100%;
  border-radius: 50%;

}

.home-card>.card>view:nth-of-type(1)>view:nth-of-type(2) {
  font-size: 26rpx;
  flex: 1;
}

.home-card>.card>view:nth-of-type(1)>view:nth-of-type(2) image {
  padding-top: 10rpx;
  width: 28rpx;
  height: 28rpx;
}

.home-card>.card>view:nth-of-type(1)>view:nth-of-type(2)>view:nth-of-type(1) {
  padding: 18rpx 0;
}

.home-card>view:nth-of-type(2)>view:nth-of-type(2) {
  display: flex;
  justify-content: space-between;
}

.home-card>view:nth-of-type(2)>view:nth-of-type(3) {
  display: flex;
  justify-content: space-between;
}

.home-card>.card>view:nth-of-type(2)>view {
  width: 280rpx;
  height: 80rpx;
  background-color: rgba(255, 255, 255, 0.3);
  display: flex;
  border-radius: 40rpx;
}

.home-card>.card>view:nth-of-type(2)>view>view {
  flex: 1;
  line-height: 80rpx;
}

.home-card>.card>view:nth-of-type(2)>view image {
  width: 78rpx;
  height: 80rpx;
}

.home-card>.card view:nth-of-type(3) view {
  flex: 1;
}


.home-card .main {
  width: 690rpx;
  box-shadow: 0px 0px 20px 0px rgba(4, 4, 155, 0.2);
  border-radius: 30rpx;
  box-sizing: border-box;
  padding: 30rpx 30rpx 48rpx;
  -webkit-overflow-scrolling: touch;
}

.home-card .main>view:nth-of-type(1) {
  display: flex;
  padding-bottom: 40rpx;
  border-bottom: 1px solid #eee;
}

.home-card .main>view:nth-of-type(1)>view:nth-of-type(1) {
  width: 140rpx;
  height: 140rpx;
  margin-right: 18rpx;
}

.home-card .main>view:nth-of-type(1)>view:nth-of-type(1)>image {
  width: 100%;
  height: 100%;
  border-radius: 50%;

}

.home-card .main>view:nth-of-type(1)>view:nth-of-type(2) {
  font-size: 26rpx;
  flex: 1;
}

.home-card .main>view:nth-of-type(1)>view:nth-of-type(2) image {
  padding-top: 10rpx;
  width: 28rpx;
  height: 28rpx;
}

.home-card .main>view:nth-of-type(1)>view:nth-of-type(2)>view:nth-of-type(1) {
  padding: 18rpx 0;
}

.home-card .main>view:nth-of-type(1)>view:nth-of-type(2)>view:nth-of-type(2) {
  display: flex;
  padding-bottom: 20rpx;
}

.home-card .main .subhead {
  display: flex;
  justify-content: space-between;
  margin: 22rpx 0;
}

.home-card .main .subhead>view:nth-of-type(1) {
  padding-left: 16rpx;
  border-left: 2px solid #2557FF;
}

.home-card .main .scroll-view {
  white-space: nowrap;
  width: 100%;
}
.home-card .main .scroll-view .scroll-view-item,.default {
  width: 300rpx;
  height: 200rpx;
  display: inline-block;
  background: #E9EDFF;
  border-radius: 20rpx;
  margin:0 20rpx 10rpx 0;
}

.home-card .main .scroll-view .scroll-view-item>image {
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}

.home-card .main .scroll-view .default image {
  width: 92rpx;
  height: 95rpx;
  margin: 33rpx 0 17rpx;
}

.home-card .main .scroll-view_H {
  white-space: nowrap;
  width: 100%;
}


.home-card .main .scroll-view_H .scroll-view-item_H {
  width: 250rpx;
  height: 250rpx;
  background: #FFFFFF;
  border: 1px solid #E9EDFF;
  display: inline-block;
  box-shadow: 0px 0px 10rpx 0px rgba(12, 47, 166, 0.1);
  border-radius: 20rpx;
  margin: 0 30rpx 50rpx 0;
}

.home-card .main  .scroll-view_H  .scroll-view-item_H  image {
  display: block;
  width: 84rpx;
  height: 84rpx;
  margin: 45rpx auto;
}

.home-card .main .follow {
  display: flex;
  justify-content: space-between;
  color: #fff;
}

.home-card .main .follow >view {
  width: 290rpx;
  line-height: 80rpx;
  background: linear-gradient(90deg, #00C0FF 0%, #0444FF 100%);
  border-radius: 40rpx;
}

.home-card .main .follow>view:nth-of-type(2) {
  background: linear-gradient(90deg, #FF039D 0%, #FF4800 100%);
}

.home-card .main .follow >view image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 30rpx;

}
.home-card .main  ._image image{
width: 50rpx;
height: 30rpx;
}

.home-card>.visitingCard {
  display: flex;
  height: 318rpx;
  width: 100%;
  box-shadow: 0px 1px 27rpx 0px rgba(10, 2, 5, 0.19);
  border-radius: 30rpx;
}

.home-card>.visitingCard>view:nth-of-type(1) {
  width: 254rpx;
  height: 100%;
}

.home-card>.visitingCard>view:nth-of-type(1) image {
  width: 191rpx;
  height: 191rpx;
  border-radius: 50%;
  margin: 40rpx 0 19rpx;
}

.home-card>.visitingCard>view:nth-of-type(2) {
  position: relative;
  width: 436rpx;
  height: 100%;
  padding: 57rpx 52rpx 52rpx;
  box-sizing: border-box;
  color: #fff;
}

.home-card>.visitingCard>view:nth-of-type(2)>image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}

.home-card>.visitingCard>view:nth-of-type(2)>view:nth-of-type(1) {
  padding-bottom: 60rpx;
}

.home-card>.visitingCard>view:nth-of-type(2) view image {
  width: 26rpx;
  height: 26rpx;
  padding-bottom: 4rpx;
  vertical-align: bottom;
}