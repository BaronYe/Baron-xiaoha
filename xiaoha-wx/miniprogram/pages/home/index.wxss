canvas {
        position: fixed;
        top: 0;
        left: 500px;
}

.home-container>view:nth-last-of-type(2) {
        position: fixed;
        left: 30rpx;
        bottom: 40rpx;
        width: 84rpx;
        line-height: 84rpx;
        border-radius: 50%;
        box-shadow: 1px 1px 30px 0px rgba(71, 111, 244, 0.3);
        transition: all 3s ease 0;
}

.home-container>view:nth-last-of-type(1) {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-color: rgba(0, 0, 0, 0.3);
}

.home-container>view:nth-last-of-type(1)>view {
        position: absolute;
        left: 30rpx;
        bottom: 40rpx;
        transition: all 3s ease 0;
}

.home-container>view:nth-last-of-type(1)>view view:nth-of-type(1) view:nth-of-type(2) {
        line-height: 84rpx;
        height: 84rpx;
        padding-left: 40rpx;
        color: #fff;

}

.home-container>view:nth-last-of-type(1)>view>view>view {
        display: inline-block;
}

.home-container>view:nth-last-of-type(1)>view view:nth-of-type(2) view:nth-of-type(2) {
        line-height: 84rpx;
        padding-left: 40rpx;
        color: #fff;
}

.home-container>view:nth-last-of-type(1)>view view:nth-of-type(3) {
        width: 84rpx;
        line-height: 84rpx;
        border-radius: 50%;
        box-shadow: 1px 1px 30px 0px rgba(71, 111, 244, 0.3);
        background-color: #fff;
}

.home-container>view:nth-last-of-type(1)>view view image {
        width: 84rpx;
        height: 84rpx;
        vertical-align: middle;
}

.home-container>.card {
        width: 160rpx;
        line-height: 60rpx;
        background: #FFFFFF;
        box-shadow: 0px 2rpx 10rpx 0px rgba(95, 15, 229, 0.1);
        border-radius: 30rpx;
        float: right;
        margin: 30rpx 30rpx 0 0;
}

.home-container {
        height: 100vh;
        width: 100vw;
        color: #333;
        /* background-color: #F2F2F2; */
        position: relative;
}

.home-container .closure {
        animation: closure .6s ease-in;
        animation-direction: reverse;
}

.home-container .begin {
        animation: slideUp .6s ease-in-out;
        animation-direction: reverse;
}

@keyframes slideUp {
        0% {}

        70% {
                opacity: 1;
        }

        100% {
                transform: rotate(15deg) translateX(1000rpx);
                opacity: 0;
        }
}

@keyframes closure {
        0% {}

        70% {
                opacity: 1;
        }

        100% {
                transform: rotate(-10deg) translateX(-1000rpx);
                opacity: 0;
        }
}

.home-container .container {
        position: absolute;
        left: 30rpx;
        top: 30rpx;
}

.home-container .particulars {
        position: absolute;
        width: 690rpx;
        height: 660rpx;
        background: #FFFFFF;
        box-shadow: 1px 1px 30px 0px rgba(71, 111, 244, 0.3);
        border-radius: 40rpx;
        box-sizing: border-box;
        padding: 30rpx 30rpx 48rpx;
        -webkit-overflow-scrolling: touch;
}

.home-container .particulars>view:nth-last-of-type(1) {
        position: absolute;
        top: 40rpx;
        left: 40rpx;
        width: 610rpx;
        height: 660rpx;
        background: #FFFFFF;
        box-shadow: 0px 1px 16rpx 0px rgba(12, 47, 166, 0.1);
        opacity: 0.3;
        border-radius: 40rpx;
        z-index: -2;
}

.home-container .particulars>view:nth-last-of-type(2) {
        position: absolute;
        top: 20rpx;
        left: 20rpx;
        z-index: -1;
        width: 650rpx;
        height: 660rpx;
        background: #FFFFFF;
        box-shadow: 0px 1px 16rpx 0px rgba(12, 47, 166, 0.1);
        opacity: 0.6;
        border-radius: 40rpx;
}

.home-container .particulars>view:nth-of-type(1) {
        position: absolute;
        top: 0;
        left: 50%;
        transform: translate(-50%, -50%);
        width: 200rpx;
        height: 200rpx;
}

.home-container .particulars>view:nth-of-type(1)>view {
        position: absolute;
        right: 0;
        bottom: 0;
}

.home-container .particulars>view:nth-of-type(1)>view>image {
        width: 46rpx;
        height: 46rpx;
}

.home-container .particulars>view:nth-of-type(1)>image {
        width: 100%;
        height: 100%;
        border-radius: 50%;
}

.home-container .particulars>view:nth-of-type(2) {
        position: absolute;
        bottom: 0;
        left: 50%;
        transform: translate(-50%, 50%);
        width: 300rpx;
        line-height: 100rpx;
        color: #fff;
        background: linear-gradient(90deg, #02C0FF 0%, #285AFF 100%);
        border-radius: 50px;
}

.home-container .particulars>view:nth-of-type(3) {
        display: flex;
        justify-content: space-between;
        padding-bottom: 94rpx;
        color: #343434;
}

.home-container .particulars>view:nth-of-type(3) image {
        width: 20rpx;
        height: 24rpx;
}

.home-container .particulars>view:nth-of-type(5) {
        line-height: 40rpx;
}

.home-container .particulars>view:nth-of-type(7) {
        margin: auto;
        color: #343434;
        display: flex;
}

.home-container .particulars>view:nth-of-type(7)>view {
        flex: 1;
}