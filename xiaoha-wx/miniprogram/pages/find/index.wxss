.find-container {
  padding: 30rpx;
}

.find-container>view {
  box-sizing: border-box;
  line-height: 142rpx;
  display: flex;
}
.find-container>view>view:nth-of-type(1){
  width: 90rpx;
  height: 90rpx;
  padding-top: 28rpx;
}
.find-container>view>view:nth-of-type(1)>image {
  width: 100%;
  height: 100%;
  
}
.find-container>view>view:nth-of-type(2) {
  flex: 1;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #eee;
}
.find-container>view>view:nth-of-type(2) >view:nth-of-type(2) >image{
  width: 14rpx;
  height: 24rpx;
}
.find-container>button {
  box-sizing: border-box;
  line-height: 142rpx;
  display: flex;
  background-color: #fff;
  padding: 0;
}
.find-container>button>view:nth-of-type(1){
  width: 90rpx;
  height: 90rpx;
  padding-top: 28rpx;
}
.find-container>button>view:nth-of-type(1)>image {
  width: 100%;
  height: 100%;
  
}
.find-container>button>view:nth-of-type(2) {
  flex: 1;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #eee;
}
.find-container>button>view:nth-of-type(2) >view:nth-of-type(2) >image{
  width: 14rpx;
  height: 24rpx;
}