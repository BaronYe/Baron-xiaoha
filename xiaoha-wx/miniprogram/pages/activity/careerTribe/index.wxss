.careerTribe_container{
  width:100%;
  height:100%;
  padding:0 10rpx;
  box-sizing: border-box;

}
.careerTribe_container .partition {
  display: flex;
  justify-content: space-between;
  line-height: 80rpx;
  height: 80rpx;
  margin-top: 20rpx;
}
.careerTribe_container .partition>view:nth-child(1) {
  padding:0 15rpx;
}
.careerTribe_container .partition image {
  width: 36rpx;
  height: 37rpx;
  padding: 20rpx;
}

.careerTribe_container._main .scroll-view_H {
  white-space: nowrap;
  width: 100%;

}

.careerTribe_container ._main .scroll-view_H .uni-scroll-view::-webkit-scrollbar {
  width: 0;
}

.careerTribe_container ._main .scroll-view_H .scroll-view-item_H {
  width: 220rpx;
  height: 250rpx;
  background: #FFFFFF;
  /* border: 1px solid #E9EDFF; */
  box-shadow: 0px 0px 10rpx 0px rgba(12, 47, 166, 0.1);
  border-radius: 20rpx;
  display: inline-block;
  box-sizing: border-box;
  margin: 20rpx 0 20rpx 18rpx;
  text-align: center;
}
.careerTribe_container ._main .scroll-view_H .scroll-view-item_H>._img {

  width: 112rpx;
  height: 112rpx;
  display: block;
  margin: 25rpx auto;
}
.scroll-view-item_H >view{
  width:99%;
  margin:0 auto;
  overflow: hidden;white-space: nowrap;text-overflow: ellipsis;
  font-size: 28rpx;
  line-height: 60rpx;
}

