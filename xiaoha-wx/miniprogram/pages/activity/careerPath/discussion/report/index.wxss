.report{
  background-color: #F2F2F2;
  height: 100vh;
}
.report>view:nth-of-type(1) {
  margin: 0 40rpx;
  line-height: 60rpx;
}
.report>view:nth-of-type(2) {
  background-color: #fff;
}
.report>view:nth-of-type(2) view {
  line-height: 100rpx;
  font-size: 36rpx;
  font-weight: 900;
  margin: 0 40rpx;
  border-bottom: 1px solid #eee;
}
