
.activity-container   {
  color: #fff;
  padding: 40rpx  30rpx ;
}
.activity-container   > view > view:nth-of-type(1) {
  height: 40rpx;
}
.activity-container   .tj-col image{
  width: 36rpx;
  height: 36rpx;
}
.activity-container   > view{
  position: relative;
}
.activity-container   > view  ._img{
  position: absolute;
  top: 0;
  left: 0;
  width: 690rpx;
  height: 86%;
  z-index: -1;
}
.activity-container   > view .tj-col>view:nth-last-child(1){
  padding: 30rpx 58rpx 0;
}



