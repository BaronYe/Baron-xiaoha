  padding-right: 60rpx;
}
/* 弹框的样式 */
uni-modal .uni-modal {
  position: fixed;
  z-index: 999;
  width: 80%;
  max-width: 300px;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #fff;
  text-align: center;
  border-radius: 20px;
  overflow: hidden;
}
.tj-panel {
  padding: 15px;
}
.tj-panel.no-padding {
  padding: 0;
}
.tj-panel.is-round {
  border-radius: 5px;
}
.tj-panel.is-shadow {
  box-shadow: 0px 6px 7px 0px #eee;
}
.tj-panel.is-gutter {
  margin-left: 15px;
  margin-right: 15px;
}
.tj-panel.tj-panel--dark {
  color: #333;
  background: #eee;
}
.tj-panel.tj-panel--light {
  background: #fff;
}
.tj-panel.tj-panel--plain {
  border: 1px solid #eee;
  background: #fff;
}
.tj-row {
  width: 100%;
  overflow: hidden;
}
.tj-row .tj-row__inner:before {
  content: '';
  display: table;
}
.tj-row .tj-row__inner::after {
  content: '';
  display: table;
  clear: both;
}
.tj-col {
  box-sizing: border-box;
  line-height: 19px;
}
.tj-col.is-vertical:after {
  content: '';
  width: 0;
  height: 100%;
  display: inline-block;
  vertical-align: middle;
}
.tj-col.tj-col-2 {
  width: 16.66%;
  float: left;
}
.tj-col.tj-col-3 {
  width: 25%;
  float: left;
}
.tj-col.tj-col-4 {
  width: 33.33%;
  float: left;
}
.tj-col.tj-col-5 {
  width: 41.66%;
  float: left;
}
.tj-col.tj-col-6 {
  width: 50%;
  float: left;
}
.tj-col.tj-col-7 {
  width: 58.33%;
  float: left;
}
.tj-col.tj-col-8 {
  width: 66.66%;
  float: left;
}
.tj-cell .tj-cell__inner {
  background: #fff;
  color: #333;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 45px;
  line-height: 45px;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.tj-cell .tj-cell__inner .tj-cell__icon {
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 20px;
          flex: 0 0 20px;
  margin-left: 10px;
}
.tj-cell .tj-cell__inner .tj-cell__title {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.tj-cell .tj-cell__inner .tj-cell__value {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  width: 55%;
  text-align: right;
}
.tj-cell .tj-cell__inner .tj-cell__value.is-gray {
  color: #999;
}
.tj-cell .tj-cell__inner .tj-cell__right {
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 20px;
          flex: 0 0 20px;
  color: #ccc;
  text-align: right;
}
.tj-popup {
  background: #fff;
  position: fixed;
  width: 100%;
  z-index: 1;
  -webkit-transition: all 1s linear;
  transition: all 1s linear;
}
.tj-popup.is-top {
  left: 0;
  top: 0;
}
.tj-popup.move-enter-active, .tj-popup.move-leave-active {
  -webkit-transform: translate3d(0, 0, 0);
          transform: translate3d(0, 0, 0);
}
.tj-popup.move-enter, .tj-popup.move-leave-active {
  -webkit-transform: translate3d(0, -100%, 0);
          transform: translate3d(0, -100%, 0);
}
.tj-popup.is-right {
  width: 70%;
  overflow-y: auto;
  height: 100%;
  right: 0;
  top: 0;
}
.tj-popup.move-enter-active, .tj-popup.move-leave-active {
  -webkit-transform: translate3d(0, 0, 0);
          transform: translate3d(0, 0, 0);
}
.tj-popup.move-enter, .tj-popup.move-leave-active {
  -webkit-transform: translate3d(100%, 0, 0);
          transform: translate3d(100%, 0, 0);
}
.tj-popup.is-bottom {
  left: 0;
  bottom: 0;
}
.tj-popup.move-enter-active, .tj-popup.move-leave-active {
  -webkit-transform: translate3d(0, 0, 0);
          transform: translate3d(0, 0, 0);
}
.tj-popup.move-enter, .tj-popup.move-leave-active {
  -webkit-transform: translate3d(0, 100%, 0);
          transform: translate3d(0, 100%, 0);
}
.tj-popup .tj-popup__main {
  max-height: 300px;
  overflow-y: auto;
}
.tj-popup.is-left {
  left: 0;
  top: 0;
}
.tj-popup.move-enter-active, .tj-popup.move-leave-active {
  -webkit-transform: translate3d(0, 0, 0);
          transform: translate3d(0, 0, 0);
}
.tj-popup.move-enter, .tj-popup.move-leave-active {
  -webkit-transform: translate3d(-100%, 0, 0);
          transform: translate3d(-100%, 0, 0);
}
.tj-popup .tj-popup__head {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 15px 15px 0;
}
.tj-popup .tj-popup__title {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  font-weight: bolder;
  color: #333;
  font-size: 16px;
}
.tj-popup .tj-popup__btn {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: right;
}
.tj-popup .tj-popup__main {
  padding: 15px;
}

